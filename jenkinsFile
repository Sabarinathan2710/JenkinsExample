pipeline{
    agent any
        tools{
            jdk 'JDK 21'
            maven 'Maven'
        }
        stages{
            stage('Build Maven'){
                steps{
                    checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/Sabarinathan2710/JenkinsExample.git']])
                    bat 'mvn clean install'
                }
            }
           
        }
}